page{  
  display: flex;  
  background-color: #fdfdfd;
  flex-direction: column;  
  height: 100%;  
}  
.navbar{   
  height: 40px;
  width: 100%;
  background: #FFFFFF;  
  z-index: 9999999;
  position: fixed;
  top:0;

}  
.navbar .item{  
  position: relative;  
  flex: auto;  
  text-align: center;  
  line-height: 50rpx; 
  height:25px;
  width:80px;
  font-size: 12px; 
  
}  
.navbar .item.active{  
  color: #F54844;  
  
  background: white;
}  
.navbar .item.active:after{  
  content: "";  
  display: block;  
  position: absolute;  
  bottom: 0;  
  left: 0;  
  right: 0;  
  height: 4rpx;   
  border-width: 0px;
}  


icon{
    margin-right: 13px;
}
.page__bd { 
    text-align: left; 
}
.icon-box{
    margin-bottom: 25px;
    display: flex;
    align-items: center;
}
.icon-box__ctn{
    flex-shrink: 100;
}
.icon-box__title{
    font-size: 20px;
}
.icon-box__desc{
    margin-top: 6px;
    font-size: 12px;
    color: #888888;
}
.icon_sp_area {
    margin-top: 10px;
    text-align: left;
}

